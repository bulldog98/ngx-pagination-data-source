{
  "compilerOptions": {
    "lib": ["es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../dist/ngx-pagination-data-source/schematics",
    "noEmitOnError": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "skipDefaultLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "declaration": true,
    "target": "es6",
    "types": [
      "jasmine",
      "node"
    ],
    "baseUrl": ".",
    "rootDir": "schematics"
  },
  "include": [
    "schematics/**/*"
  ],
  "exclude": [
    "**/*.spec.ts"
  ]
}
